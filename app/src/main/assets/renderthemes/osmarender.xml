<rendertheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" map-background="#e3e1d2"
  map-background-outside="#e3e1d2" version="5" xmlns="http://mapsforge.org/renderTheme"
  xsi:schemaLocation="http://mapsforge.org/renderTheme renderTheme-v5.xsd">

  <rule e="way" k="natural" v="issea|sea">
    <area fill="#B3DDFF"/>
  </rule>

  <rule e="way" k="natural" v="nosea">
    <area fill="#e3e1d2"/>
  </rule>

  <!-- ways -->
  <rule e="way" k="*" v="*">
    <!-- landuse -->
    <rule e="way" k="landuse" v="*">
      <rule e="way" k="landuse" v="military">
        <area fill="#b2c470"/>
      </rule>

      <rule e="way" k="landuse" v="residential|farmyard">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="retail">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="industrial|brownfield|railway">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="commercial">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="construction|greenfield">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="garages">
        <area fill="#e3e1d2"/>
      </rule>

      <rule e="way" k="*" v="*" zoom-min="16">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- tunnel -->
    <rule e="way" k="tunnel" v="true|yes">
      <!-- highway tunnels -->
      <rule e="way" k="highway" v="*">
        <!-- highway tunnel casings -->
        <rule e="way" k="highway" v="steps|footway|path">
          <line stroke="#707070" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="0.6"/>
        </rule>
        <rule e="way" k="highway" v="track|cycleway|bridleway|service">
          <line stroke="#707070" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.0"/>
        </rule>
        <rule e="way" k="highway" v="construction">
          <line stroke="#707070" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.1"/>
        </rule>
        <rule e="way" k="highway" v="road">
          <line stroke="#707070" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.3"/>
        </rule>
        <rule e="way" k="highway"
          v="pedestrian|unclassified|residential|living_street|byway">
          <line stroke="#707070" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.5"/>
        </rule>
        <rule e="way" k="highway"
          v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
          <line stroke="#707070" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.8"/>
        </rule>
        <rule e="way" k="highway" v="motorway">
          <line stroke="#707070" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="2.0"/>
        </rule>

        <!-- highway tunnel cores -->
        <rule e="way" k="highway" v="steps|footway|path">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="0.45"/>
        </rule>
        <rule e="way" k="highway" v="track">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="0.7"/>
        </rule>
        <rule e="way" k="highway" v="cycleway|bridleway|service">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="0.85"/>
        </rule>
        <rule e="way" k="highway" v="construction">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.0"/>
        </rule>
        <rule e="way" k="highway" v="road">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.25"/>
        </rule>
        <rule e="way" k="highway"
          v="pedestrian|unclassified|residential|living_street|byway">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.35"/>
        </rule>
        <rule e="way" k="highway"
          v="tertiary_link|tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.5"/>
        </rule>
        <rule e="way" k="highway" v="motorway">
          <line stroke="#ffffff" stroke-dasharray="5,5" stroke-linecap="butt"
            stroke-width="1.7"/>
        </rule>
      </rule>

      <!-- railway tunnel -->
      <rule e="way" k="railway" v="*">
        <rule e="way" k="railway" v="tram|subway|light_rail|narrow_gauge">
          <line stroke="#880f0f4c" stroke-dasharray="4,4" stroke-linecap="butt"
            stroke-width="0.25"/>
        </rule>
        <rule e="way" k="railway" v="rail">
          <line stroke="#aa333333" stroke-dasharray="4,4" stroke-linecap="butt"
            stroke-width="0.4"/>
        </rule>
      </rule>
    </rule>


    <!-- landuse -->
    <rule e="way" k="landuse" v="*">
      <rule e="way" k="landuse" v="landfill|quarry">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="cemetery">
        <area fill="#b2c470"/>
      </rule>
      <rule e="way" k="landuse" v="field|farm|farmland|meadow|grass">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="allotments|village_green|recreation_ground">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="landuse" v="reservoir|basin">
        <area fill="#8BD3E0"/>
      </rule>

      <rule e="way" k="*" v="*" zoom-min="16">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <rule e="way" k="natural|landuse" v="forest|wood|scrub">
      <rule e="way" k="wood" v="*">
        <rule e="way" k="wood" v="coniferous">
          <area fill="#B0C270"/>
        </rule>
        <rule e="way" k="wood" v="deciduous">
          <area fill="#B0C270"/>
        </rule>
        <rule e="way" k="wood" v="mixed">
          <area fill="#B0C270"/>
        </rule>
      </rule>
      <rule e="way" k="wood" v="~">
        <area fill="#B0C270"/>
      </rule>

      <rule e="way" k="*" v="*" zoom-min="16">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- amenity -->
    <rule e="way" k="amenity" v="*">
      <rule e="way" k="amenity" v="kindergarten|school|college|university">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="amenity" v="grave_yard">
        <area fill="#b2c470"/>
      </rule>
      <!--<rule e="way" k="amenity" v="parking">-->
      <!--<area fill="#ffffdf" />-->
      <!--<rule e="way" k="*" v="*" zoom-min="17">-->
      <!--<symbol src="assets:symbols/parking.svg" />-->
      <!--</rule>-->
      <!--</rule>-->
      <rule closed="yes" e="way" k="amenity" v="fountain">
        <area fill="#e3e1d2"/>
        <rule e="way" k="*" v="*" zoom-min="18">
          <symbol src="assets:symbols/fountain.svg"/>
        </rule>
      </rule>
      <rule e="way" k="amenity" v="hospital" zoom-min="17">
        <symbol src="assets:symbols/clinic.svg"/>
      </rule>
      <rule e="way" k="amenity" v="theatre" zoom-min="17">
        <symbol src="assets:symbols/theatre.svg"/>
      </rule>
      <rule e="way" k="amenity" v="toilets" zoom-min="17">
        <symbol src="assets:symbols/toilets.svg"/>
      </rule>

      <rule e="way" k="*" v="*" zoom-min="17">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- natural -->
    <rule e="way" k="natural" v="*">
      <rule e="way" k="natural" v="glacier">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="natural" v="land">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="natural" v="beach">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="natural" v="heath">
        <area fill="#e3e1d2"/>
      </rule>

      <rule e="way" k="*" v="*" zoom-min="16">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>

    <!-- leisure -->
    <rule e="way" k="leisure" v="*">
      <rule e="way" k="leisure" v="park|garden|golf_course|common|green">
        <area fill="#b2c470"/>
      </rule>

      <rule e="way" k="leisure" v="stadium|sports_centre|water_park" zoom-min="14">
        <area fill="#cccabd"/>
      </rule>
      <rule e="way" k="leisure" v="track" zoom-min="15">
        <rule e="way" k="area" v="yes|true">
          <area fill="#e3e1d2"/>
        </rule>
        <rule e="way" k="area" v="~|no|false">
          <line stroke="#9e9d93" stroke-width="0.75"/>
          <line stroke="#9e9d93" stroke-width="0.7"/>
        </rule>
      </rule>
      <rule e="way" k="leisure" v="swimming_pool">
        <area fill="#8BD3E0"/>
      </rule>

      <rule e="way" k="*" v="*" zoom-min="16">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- waterways -->
    <rule e="way" k="waterway" v="*">
      <!-- waterway casings -->
      <rule e="way" k="waterway" v="drain">
        <line stroke="#b0b0b0" stroke-width="1.0"/>
      </rule>
      <rule e="way" k="waterway" v="canal">
        <line stroke="#b0b0b0" stroke-width="1.0"/>
      </rule>

      <!-- waterway cores -->
      <rule e="way" k="waterway" v="ditch">
        <line stroke="#8BD3E0" stroke-width="0.1"/>
      </rule>
      <rule e="way" k="waterway" v="drain">
        <line stroke="#8BD3E0" stroke-width="0.8"/>
      </rule>
      <rule e="way" k="waterway" v="canal">
        <line stroke="#8BD3E0" stroke-width="0.8"/>
      </rule>
      <rule e="way" k="waterway" v="stream">
        <line stroke="#8BD3E0" stroke-width="0.275"/>
      </rule>
      <rule e="way" k="waterway" v="river">
        <line stroke="#8BD3E0" stroke-width="1.0"/>
      </rule>

      <rule e="way" k="waterway" v="dock">
        <area fill="#8BD3E0"/>
      </rule>
      <rule e="way" k="waterway" v="riverbank">
        <area fill="#8BD3E0"/>
      </rule>
      <rule e="way" k="waterway" v="weir">
        <line stroke="#000044" stroke-width="0.375"/>
      </rule>
      <rule e="way" k="waterway" v="dam">
        <line stroke="#000000" stroke-width="0.5"/>
      </rule>
      <rule e="way" k="lock" v="yes|true">
        <line stroke="#000000" stroke-linecap="butt" stroke-width="2.0"/>
        <line stroke="#f8f8f8" stroke-linecap="butt" stroke-width="1.5"/>
      </rule>
    </rule>


    <!-- military -->
    <rule e="way" k="military" v="*">
      <area fill="#e3e1d2"/>
    </rule>


    <!-- tourism areas -->
    <rule e="way" k="tourism" v="*">
      <rule e="way" k="tourism" v="attraction">
        <area fill="#e3e1d2"/>
      </rule>
      <rule e="way" k="tourism" v="zoo|picnic_site|caravan_site|camp_site">
        <area fill="#b2c470"/>
      </rule>

      <rule e="way" k="*" v="*" zoom-min="16">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <rule e="way" k="natural" v="water">
      <area fill="#8BD3E0"/>
    </rule>


    <!-- highway area -->
    <rule e="way" k="area" v="yes|true">
      <rule e="way" k="highway" v="*">
        <rule e="way" k="highway" v="footway|footpath">
          <area fill="#EDE8D6"/>
        </rule>
        <rule e="way" k="highway" v="pedestrian">
          <area fill="#EDE8D6"/>
        </rule>
        <rule e="way" k="highway" v="path">
          <area fill="#EDE8D6"/>
        </rule>
        <rule e="way" k="highway" v="service">
          <area fill="#ffffff"/>
        </rule>
        <rule e="way" k="highway" v="unclassified">
          <area fill="#ffffff"/>
        </rule>
        <rule e="way" k="highway" v="residential">
          <area fill="#ffffff"/>
        </rule>
        <rule e="way" k="highway" v="road">
          <area fill="#d0d0d0"/>
        </rule>
        <rule e="way" k="highway" v="living_street">
          <area fill="#ffffff" stroke="#ffffff"/>
        </rule>
      </rule>
    </rule>


    <!-- platform cores -->
    <rule e="way" k="highway|railway|public_transport" v="platform">
      <rule closed="yes" e="way" k="*" v="*">
        <area fill="#EDE8D6"/>
      </rule>
      <rule closed="no" e="way" k="*" v="*">
        <line stroke="#EDE8D6" stroke-width="0.3"/>
      </rule>
    </rule>


    <!-- runways casings and areas -->
    <rule e="way" k="aeroway" v="*">
      <rule closed="yes" e="way" k="aeroway" v="aerodrome" zoom-min="11">
        <area fill="#dcced8"/>
      </rule>
      <rule e="way" k="aeroway" v="apron" zoom-min="11">
        <area fill="#dcced8"/>
      </rule>
      <rule e="way" k="aeroway" v="terminal" zoom-min="11">
        <area fill="#dcced8"/>
      </rule>
      <rule e="way" k="*" v="*" zoom-min="16">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>
  </rule>


  <!-- turning circles -->
  <rule e="node" k="highway" v="turning_circle">
    <circle fill="#707070" radius="1.5" scale-radius="true"/>
  </rule>


  <!-- ways -->
  <rule e="way" k="*" v="*">

    <!-- building -->
    <rule e="way" k="building" v="*">
      <area fill="#CBC9BB"/>

      <rule e="way" k="*" v="*" zoom-min="17">
        <caption fill="#606060" font-size="10" k="name"/>
        <caption fill="#606060" font-size="10" k="addr:housenumber"/>
      </rule>
    </rule>


    <!-- roads -->

    <rule e="way" k="highway" v="*">
      <!-- no tunnel -->
      <rule e="way" k="tunnel" v="~|no|false">
        <!-- no area -->
        <rule e="way" k="area" v="~|no|false">
          <!-- highway cores -->
          <!-- пешеходная дорога -->
          <rule e="way" k="highway" v="steps" zoom-min="16">
            <line stroke="#FFFFFF" stroke-dasharray="3,1" stroke-linecap="butt"
              stroke-width="0.15"/>
            <rule e="way" k="*" v="*" zoom-min="16">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <!-- пешеходная дорога -->
          <rule e="way" k="highway" v="footway" zoom-min="16">
            <line stroke="#FFFFFF" stroke-dasharray="5,2" stroke-linecap="butt"
              stroke-width="0.1"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="path">
            <line stroke="#d0d0d0" stroke-width="0.45"/>
            <rule e="way" k="*" v="*" zoom-min="16">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <!-- походу лесные дороги 5шт -->
          <rule e="way" k="highway" v="track">
            <rule e="way" k="tracktype" v="grade1">
              <line stroke="#FFFFFF" stroke-width="0.75"/>
              <rule e="way" k="*" v="*" zoom-min="15">
                <pathText font-size="10" font-style="normal" k="name"
                  stroke="#ffffff" stroke-width="2.0"/>
              </rule>
            </rule>
            <rule e="way" k="tracktype" v="grade2">
              <line stroke="#FFFFFF" stroke-width="0.75"/>
              <rule e="way" k="*" v="*" zoom-min="15">
                <pathText font-size="10" font-style="normal" k="name"
                  stroke="#ffffff" stroke-width="2.0"/>
              </rule>
            </rule>
            <rule e="way" k="tracktype" v="grade3">
              <line stroke="#FFFFFF" stroke-width="0.7"/>
              <rule e="way" k="*" v="*" zoom-min="15">
                <pathText font-size="10" font-style="normal" k="name"
                  stroke="#ffffff" stroke-width="2.0"/>
              </rule>
            </rule>
            <rule e="way" k="tracktype" v="grade4">
              <line stroke="#FFFFFF" stroke-width="0.7"/>
              <rule e="way" k="*" v="*" zoom-min="15">
                <pathText font-size="10" font-style="normal" k="name"
                  stroke="#ffffff" stroke-width="2.0"/>
              </rule>
            </rule>
            <rule e="way" k="tracktype" v="grade5">
              <line stroke="#FFFFFF" stroke-width="0.7"/>
              <rule e="way" k="*" v="*" zoom-min="15">
                <pathText font-size="10" font-style="normal" k="name"
                  stroke="#ffffff" stroke-width="2.0"/>
              </rule>
            </rule>
            <rule e="way" k="tracktype" v="~|no|false">
              <line stroke="#FFFFFF" stroke-width="0.75"/>
              <rule e="way" k="*" v="*" zoom-min="15">
                <pathText font-size="10" font-style="normal" k="name"
                  stroke="#ffffff" stroke-width="2.0"/>
              </rule>
            </rule>
          </rule>

          <!-- опять пешеходная. через парк жилибера например -->

          <rule e="way" k="highway" v="cycleway">
            <line stroke="#FFFFFF" stroke-width="0.85"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>

          <!-- для всадников -->

          <rule e="way" k="highway" v="bridleway">
            <line stroke="#FFFFFF" stroke-width="0.85"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>


          <rule e="way" k="highway" v="service">
            <line stroke="#ffffff" stroke-width="0.85"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="construction">
            <line stroke="#d0d0d0" stroke-width="1.0"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="road">
            <line stroke="#d0d0d0" stroke-width="1.25"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="pedestrian">
            <line stroke="#e5e0c2" stroke-width="1.35"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="unclassified|residential|living_street">
            <line stroke="#ffffff" stroke-width="1.35"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="byway">
            <line stroke="#efadaa" stroke-width="1.35"/>
            <rule e="way" k="*" v="*" zoom-min="15">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>

          <!-- не главная дорога. белая такая -->

          <rule e="way" k="highway" v="tertiary|tertiary_link">
            <line stroke="#FFFFFF" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>

          <!-- не главная дорога. бледно желтая -->

          <rule e="way" k="highway" v="secondary_link">
            <line stroke="#FFFFFF" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="primary_link">
            <line stroke="#FFFFFF" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="trunk_link">
            <line stroke="#FFFFFF" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="motorway_link">
            <line stroke="#FFFFFF" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>


          <!-- главная дорогая -->

          <rule e="way" k="highway" v="secondary">
            <line stroke="#FFFFFF" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="primary">
            <line stroke="#ffb36f" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
              <pathText fill="#606060" font-size="12" font-style="normal" k="ref"
                stroke="#ffffff" stroke-width="2.0"/>
            </rule>
          </rule>

          <rule e="way" k="highway" v="trunk">
            <line stroke="#FFFFFF" stroke-width="1.5"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
              <pathText fill="#606060" font-size="12" font-style="bold" k="ref"
                stroke="#ffffff" stroke-width="2.0"/>
            </rule>
          </rule>
          <rule e="way" k="highway" v="motorway">
            <line stroke="#FFFFFF" stroke-width="1.7"/>
            <rule e="way" k="*" v="*" zoom-min="14">
              <pathText font-size="10" font-style="normal" k="name" stroke="#ffffff"
                stroke-width="2.0"/>
              <pathText fill="#606060" font-size="12" font-style="bold" k="ref"
                stroke="#ffffff" stroke-width="2.0"/>
            </rule>
          </rule>

        </rule>
      </rule>
    </rule>


    <!-- runways cores -->
    <rule e="way" k="aeroway" v="*">
      <rule e="way" k="aeroway" v="runway">
        <line stroke="#e8e7db" stroke-linecap="square" stroke-width="4.5"/>
      </rule>
      <rule e="way" k="aeroway" v="taxiway">
        <line stroke="#e8e7db" stroke-linecap="butt" stroke-width="1.8"/>
      </rule>
    </rule>


    <!-- man_made features -->
    <rule e="way" k="man_made" v="pier">
      <rule closed="no" e="way" k="*" v="*">
        <line stroke="#d0d0d0" stroke-linecap="butt" stroke-width="0.4"/>
        <line stroke="#e4e4e4" stroke-linecap="butt" stroke-width="0.3"/>
      </rule>
      <rule closed="yes" e="way" k="*" v="*">
        <area fill="#e3e1d2"/>
      </rule>
    </rule>


    <!-- non-physical routes -->
    <rule e="way" k="route" v="ferry">
      <line stroke="#707070" stroke-dasharray="15,10" stroke-linecap="butt"
        stroke-width="0.3"/>
    </rule>


    <!-- highway one-way markers -->
    <rule e="way" k="tunnel" v="~|false|no">
      <rule e="way" k="area" v="~|false|no">
        <rule e="way" k="highway" v="*">
          <rule e="way" k="oneway" v="yes|true" zoom-min="16">
            <lineSymbol align-center="true" repeat="true"
              src="assets:symbols/oneway.svg" symbol-percent="80"/>
          </rule>
        </rule>
      </rule>
    </rule>


    <!-- pistes -->
    <rule e="way" k="piste:type" v="*">
      <!-- piste areas -->
      <rule closed="yes" e="way" k="piste:type" v="downhill">
        <rule e="way" k="piste:difficulty" v="novice">
          <area fill="#5540ff40" stroke="#5540ff40" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="easy">
          <area fill="#55606060" stroke="#55606060" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="intermediate">
          <area fill="#55ff4040" stroke="#55ff4040" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="advanced">
          <area fill="#55000000" stroke="#55000000" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="expert">
          <area fill="#55f6800a" stroke="#55f6800a" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="freeride">
          <area fill="#55f6dd0a" stroke="#55f6dd0a" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="~">
          <area fill="#55505050" stroke="#55505050" stroke-width="5.0"/>
        </rule>

        <rule e="way" k="*" v="*" zoom-min="16">
          <caption fill="#000000" font-size="10" font-style="bold" k="name"
            stroke="#ffffff" stroke-width="2.0"/>
        </rule>
      </rule>

      <!-- piste ways -->
      <rule closed="no" e="way" k="piste:type" v="downhill">
        <rule e="way" k="piste:difficulty" v="novice">
          <line stroke="#5540ff40" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="easy">
          <line stroke="#55606060" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="intermediate">
          <line stroke="#55ff4040" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="advanced">
          <line stroke="#55000000" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="expert">
          <line stroke="#55f6800a" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="freeride">
          <line stroke="#55f6dd0a" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="piste:difficulty" v="~">
          <line stroke="#55505050" stroke-width="5.0"/>
        </rule>
        <rule e="way" k="*" v="*" zoom-min="14">
          <pathText fill="#000000" font-size="10" font-style="bold" k="name"
            stroke="#ffffff" stroke-width="2.0"/>
        </rule>
      </rule>
      <rule e="way" k="piste:type" v="sled">
        <line stroke="#cdabde" stroke-width="2.5"/>
      </rule>
      <rule e="way" k="piste:type" v="nordic">
        <line stroke="#c00000" stroke-dasharray="30,5" stroke-linecap="butt"
          stroke-width="1.0"/>
      </rule>
    </rule>


    <!-- aerial ways -->
    <rule e="way" k="aerialway" v="*">
      <line stroke="#202020" stroke-linecap="butt" stroke-width="0.4"/>
      <line stroke="#202020" stroke-dasharray="2,200" stroke-linecap="butt"
        stroke-width="4.0"/>
      <rule e="way" k="*" v="*" zoom-min="14">
        <pathText fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- railway (no tunnel) -->
    <rule e="way" k="railway" v="*">
      <rule e="way" k="tunnel" v="~|false|no">
        <rule e="way" k="railway" v="station">
          <area fill="#9b9b79" stroke="#707070" stroke-width="0.3"/>
        </rule>

        <!-- railway bridge casings -->
        <rule e="way" k="bridge" v="yes|true">
          <rule e="way" k="railway" v="tram">
            <line stroke="#000000" stroke-linecap="butt" stroke-width="0.4"/>
          </rule>
          <rule e="way" k="railway" v="subway|light_rail|narrow_gauge">
            <line stroke="#000000" stroke-linecap="butt" stroke-width="0.6"/>
          </rule>
          <rule e="way" k="railway" v="rail">
            <line stroke="#000000" stroke-linecap="butt" stroke-width="0.8"/>
          </rule>
        </rule>

        <!-- railway casings and cores -->
        <rule e="way" k="railway" v="tram">
          <line stroke="#e6b7e6" stroke-linecap="butt" stroke-width="0.25"/>
          <line stroke="#4c0f4c" stroke-dasharray="12,18" stroke-linecap="butt"
            stroke-width="0.25"/>
        </rule>
        <rule e="way" k="railway" v="subway">
          <line stroke="#0f0f4c" stroke-linecap="butt" stroke-width="0.4"/>
          <line stroke="#b7b7e6" stroke-dasharray="12,18" stroke-linecap="butt"
            stroke-width="0.25"/>
        </rule>
        <rule e="way" k="railway" v="light_rail">
          <line stroke="#0f4c0f" stroke-linecap="butt" stroke-width="0.4"/>
          <line stroke="#b7e6e6" stroke-linecap="butt" stroke-width="0.25"/>
          <line stroke="#0f4c0f" stroke-dasharray="12,18" stroke-linecap="butt"
            stroke-width="0.25"/>
        </rule>
        <rule e="way" k="railway" v="narrow_gauge">
          <line stroke="#333333" stroke-linecap="butt" stroke-width="0.4"/>
          <line stroke="#e6e6e6" stroke-linecap="butt" stroke-width="0.25"/>
          <line stroke="#333333" stroke-dasharray="18,18" stroke-linecap="butt"
            stroke-width="0.25"/>
        </rule>
        <rule e="way" k="railway" v="rail">
          <line stroke="#666666" stroke-linecap="butt" stroke-width="0.55"/>
          <line stroke="#eeeeee" stroke-linecap="butt" stroke-width="0.4"/>
          <line stroke="#666666" stroke-dasharray="15,15" stroke-linecap="butt"
            stroke-width="0.4"/>
        </rule>
      </rule>
    </rule>


    <!-- non-physical boundaries -->
    <rule e="way" k="boundary" v="*">
      <rule e="way" k="boundary" v="national_park">
        <line stroke="#4ef94b" stroke-dasharray="15, 5, 5, 5" stroke-width="0.25"/>
      </rule>
      <rule e="way" k="boundary" v="administrative">
        <rule e="way" k="admin_level" v="*">
          <rule e="way" k="admin_level" v="11">
            <line stroke="#f9574b" stroke-dasharray="1, 5" stroke-width="0.1"/>
          </rule>
          <rule e="way" k="admin_level" v="10">
            <line stroke="#f9574b" stroke-dasharray="5, 25" stroke-width="0.1"/>
          </rule>
          <rule e="way" k="admin_level" v="9">
            <line stroke="#f9574b" stroke-dasharray="15, 15" stroke-width="0.1"/>
          </rule>
          <rule e="way" k="admin_level" v="8">
            <line stroke="#f9574b" stroke-dasharray="15, 5, 5, 5" stroke-width="0.1"/>
          </rule>
          <rule e="way" k="admin_level" v="7">
            <line stroke="#f9574b" stroke-width="0.1"/>
          </rule>
          <rule e="way" k="admin_level" v="6">
            <line stroke="#f9574b" stroke-dasharray="5, 5" stroke-width="0.25"/>
          </rule>
          <rule e="way" k="admin_level" v="5">
            <line stroke="#f9574b" stroke-dasharray="15, 15" stroke-width="0.25"/>
          </rule>
          <rule e="way" k="admin_level" v="4">
            <line stroke="#f9574b" stroke-dasharray="15, 5, 5, 5" stroke-width="0.25"/>
          </rule>
          <rule e="way" k="admin_level" v="3">
            <line stroke="#f9574b" stroke-width="0.25"/>
          </rule>
          <rule e="way" k="admin_level" v="2">
            <line stroke="#80ffff00" stroke-width="0.75"/>
            <line stroke="#f9574b" stroke-dasharray="15, 15" stroke-width="0.25"/>
          </rule>
          <rule e="way" k="admin_level" v="1">
            <line stroke="#80ffff00" stroke-width="1.75"/>
            <line stroke="#f9574b" stroke-dasharray="15, 5, 5, 5" stroke-width="0.25"/>
          </rule>
        </rule>
      </rule>
    </rule>


    <!-- historic -->
    <rule e="way" k="historic" v="ruins" zoom-min="17">
      <caption fill="#CBC9BB" font-size="10" k="name" stroke-width="2.0"/>
    </rule>


    <!-- place -->
    <rule e="way" k="place" v="locality" zoom-min="17">
      <caption fill="#000000" font-size="10" font-style="bold" k="name" stroke="#ffffff"
        stroke-width="2.0"/>
    </rule>
  </rule>


  <!-- nodes -->
  <rule e="node" k="*" v="*">
    <!-- aeroway -->
    <rule e="node" k="aeroway" v="*">
      <rule e="node" k="aeroway" v="aerodrome|airport" zoom-max="13">
        <symbol src="assets:symbols/airport.svg"/>
      </rule>
    </rule>


    <!-- amenity -->
    <rule e="node" k="amenity" v="*">
      <rule e="node" k="amenity" v="atm" zoom-min="17">
        <symbol src="assets:symbols/atm_circle.svg"/>
      </rule>
      <rule e="node" k="amenity" v="bank" zoom-min="17">
        <symbol src="assets:symbols/bank.svg"/>
      </rule>
      <rule e="node" k="amenity" v="bench" zoom-min="18">
        <symbol src="assets:symbols/bench.svg" symbol-percent="80"/>
      </rule>
      <rule e="node" k="amenity" v="bus_station" zoom-min="17">
        <symbol src="assets:symbols/bus_sta.svg"/>
      </rule>
      <rule e="node" k="amenity" v="cafe" zoom-min="17">
        <symbol src="assets:symbols/cafe.svg"/>
      </rule>
      <rule e="node" k="amenity" v="cinema" zoom-min="17">
        <symbol src="assets:symbols/cinema.svg"/>
      </rule>
      <rule e="node" k="amenity" v="fast_food" zoom-min="17">
        <symbol src="assets:symbols/fastfood.svg"/>
      </rule>
      <rule e="node" k="amenity" v="fountain" zoom-min="18">
        <symbol src="assets:symbols/fountain.svg" symbol-percent="50"/>
      </rule>
      <rule e="node" k="amenity" v="fuel" zoom-min="17">
        <symbol src="assets:symbols/fuel.svg"/>
      </rule>
      <rule e="node" k="amenity" v="hospital" zoom-min="17">
        <symbol src="assets:symbols/clinic.svg"/>
      </rule>
      <rule e="node" k="amenity" v="kindergarten" zoom-min="17">
        <symbol src="assets:symbols/kindergarten.svg"/>
      </rule>
      <!--<rule e="node" k="amenity" v="parking" zoom-min="20">-->
      <!--<symbol src="assets:symbols/parking.svg" />-->
      <!--</rule>-->
      <rule e="node" k="amenity" v="pharmacy" zoom-min="17">
        <symbol src="assets:symbols/apteka.svg"/>
      </rule>
      <rule e="node" k="amenity" v="place_of_worship" zoom-min="17">
        <rule e="node" k="denomination|religion" v="jewish">
          <symbol src="assets:symbols/synagogue.svg"/>
        </rule>
        <rule e="node" k="denomination|religion" v="muslim|moslem">
          <symbol src="assets:symbols/mosque.svg"/>
        </rule>
        <rule e="node" k="denomination|religion" v="christian">
          <symbol src="assets:symbols/church.svg"/>
        </rule>
      </rule>
      <rule e="node" k="amenity" v="post_office" zoom-min="17">
        <symbol src="assets:symbols/postoffice.svg"/>
      </rule>
      <rule e="node" k="amenity" v="pub|bar" zoom-min="17">
        <symbol src="assets:symbols/pub.svg"/>
      </rule>
      <rule e="node" k="amenity" v="restaurant" zoom-min="17">
        <symbol src="assets:symbols/restaurant.svg"/>
      </rule>
      <rule e="node" k="amenity" v="school" zoom-min="17">
        <symbol src="assets:symbols/school.svg"/>
      </rule>
      <rule e="node" k="amenity" v="theatre" zoom-min="17">
        <symbol src="assets:symbols/theatre.svg"/>
      </rule>
      <rule e="node" k="amenity" v="toilets" zoom-min="17">
        <symbol src="assets:symbols/toilets.svg"/>
      </rule>
      <rule e="node" k="amenity" v="university|college" zoom-min="17">
        <symbol src="assets:symbols/university.svg"/>
      </rule>

      <rule e="node" k="*" v="*" zoom-min="17">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- highway -->
    <rule e="node" k="highway" v="*">
      <rule e="node" k="highway" v="turning_circle">
        <circle fill="#ffffff" radius="1.4" scale-radius="true"/>
      </rule>
    </rule>


    <rule e="node" k="historic" v="*" zoom-min="18">
      <symbol id="monument" src="assets:symbols/monument.svg"/>
      <rule e="node" k="*" v="*" zoom-min="18">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" position="above"
          stroke="#ffffff" stroke-width="2.0" symbol-id="monument"/>
      </rule>
    </rule>


    <!-- place -->
    <rule e="node" k="place" v="*">
      <rule e="node" k="place" v="hamlet" zoom-min="14">
        <caption fill="#000000" font-size="18" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
      <rule e="node" k="place" v="suburb|town|village" zoom-min="14">
        <caption fill="#000000" font-size="18" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
      <rule e="node" k="place" v="city" zoom-max="13">
        <caption fill="#000000" font-size="18" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- railway -->

    <rule e="node" k="railway" v="subway">
      <rule e="node" k="railway" v="station" zoom-min="12">
        <symbol id="subway_station" src="assets:symbols/empty.svg"/>
      </rule>
    </rule>

    <rule e="node" k="railway" v="*">
      <rule e="node" k="railway" v="station" zoom-min="13">
        <symbol id="train_station" src="assets:symbols/train_station3.svg"/>
      </rule>
    </rule>

    <!-- shop -->
    <rule e="node" k="shop" v="*">
      <rule e="node" k="shop" v="supermarket|organic" zoom-min="17">
        <symbol src="assets:symbols/supermarket.svg"/>
      </rule>

      <rule e="node" k="*" v="*" zoom-min="17">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>


    <!-- tourism -->
    <rule e="node" k="tourism" v="*">
      <rule e="node" k="tourism" v="hostel" zoom-min="17">
        <symbol src="assets:symbols/hostel.svg"/>
      </rule>
      <rule e="node" k="tourism" v="hotel" zoom-min="17">
        <symbol src="assets:symbols/hotel.svg"/>
      </rule>
      <rule e="node" k="*" v="*" zoom-min="17">
        <caption fill="#606060" font-size="10" font-style="bold" k="name" stroke="#ffffff"
          stroke-width="2.0"/>
      </rule>
    </rule>
  </rule>
</rendertheme>
